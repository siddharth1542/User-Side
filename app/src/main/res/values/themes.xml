<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyProject23Sep" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/main_app_color</item>
        <item name="colorPrimaryVariant">@color/main_app_color</item>
        <item name="colorOnPrimary">@color/purple_700</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/main_app_color</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:navigationBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:navigationIcon">?attr/colorSecondary</item>

        <!-- Customize your theme here. -->
        <item name="drawerArrowStyle">@style/DrawerHamburgerStyle</item>


    </style>


</resources>